    "date-fns": "^2.0.0-beta.3",
    "element-ui": "^2.12.0",
    "highlight.js": "^9.15.8",
    "js-cookie": "2.2.0",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.22",
    "vue-clipboard2": "^0.3.0",
    "vue-codemirror": "^4.0.6",
    "vue-highlight.js": "^3.1.0",
    "vue-i18n": "^8.15.0",
    "vue-page-title": "^1.1.4",
    "vue-router": "^3.0.1",
    "vue-shortkey": "^3.1.7",
    "vue-splitpane": "^1.0.4",
    "vuedraggable": "2.23.2",
    "vuex": "^3.0.1"
